{
  "description": "Prompt an agent to generate text",
  "label": "AI - Agent",
  "category": "AI",
  "icon": {
    "name": "DataModelIcon",
    "color": "Purple"
  },
  "options": [
    {
      "info": "Select a model",
      "name": "model",
      "label": "Model",
      "meta": {
        "type": "Select",
        "default": "gpt-4",
        "validations": {
          "required": true
        },
        "values": [
          {
            "label": "GPT 3.5 turbo",
            "value": "gpt-3.5-turbo"
          },
          {
            "label": "GPT 3.5 turbo 16k",
            "value": "gpt-3.5-turbo-16k"
          },
          {
            "label": "GPT 4",
            "value": "gpt-4"
          },
          {
            "label": "GPT 4 16k",
            "value": "gpt-4-16k"
          }
        ]
      }
    },
    {
      "meta": {
        "type": "Text",
        "validations": {
          "required": true
        }
      },
      "name": "apiKey",
      "label": "API Key",
      "info": "The API key for the model"
    },
    {
      "name": "prompt",
      "label": "Prompt",
      "meta": {
        "type": "MultilineText"
      }
    },
    {
      "name": "variables",
      "label": "Variables",
      "meta": {
        "type": "Map"
      }
    },
    {
      "name": "maxTokens",
      "label": "Max New Tokens",
      "info": "The maximum number of tokens to generate.",
      "meta": {
        "type": "Number",
        "default": 256
      }
    },
    {
      "name": "temperature",
      "label": "Temperature",
      "info": "The higher the temperature, the crazier the text",
      "meta": {
        "type": "Number",
        "default": 20,
        "validations": {
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "name": "result",
      "label": "Result",
      "meta": {
        "type": "Output",
        "output": {
          "type": "Text"
        }
      }
    }
  ],
  "yields": "NONE"
}
