{
  "description": "Generate a PDF based on HTML using Prince 12.5",
  "label": "Generate PDF",
  "category": "Misc",
  "icon": {
    "name": "FilePDFIcon",
    "color": "Purple"
  },
  "options": [
    {
      "meta": {
        "type": "MultilineText",
        "validations": {
          "required": true
        }
      },
      "name": "html",
      "label": "HTML"
    },
    {
      "meta": {
        "type": "Map"
      },
      "info": "Select all the variables you want to have available in the HTML textfield",
      "name": "variables",
      "label": "Variables"
    },
    {
      "meta": {
        "type": "Model",
        "validations": {
          "required": true
        }
      },
      "name": "model",
      "label": "Model"
    },
    {
      "meta": {
        "allowedKinds": [
          "FILE",
          "IMAGE"
        ],
        "type": "Property",
        "model": "model",
        "validations": {
          "required": true
        }
      },
      "configuration": {
        "dependsOn": [
          {
            "option": "model",
            "action": "CLEAR"
          }
        ]
      },
      "name": "property",
      "label": "Property"
    },
    {
      "meta": {
        "type": "Text",
        "validations": {
          "required": true
        }
      },
      "name": "fileName",
      "label": "File name"
    },
    {
      "meta": {
        "type": "Output",
        "output": {
          "type": "Text"
        }
      },
      "info": "The file reference",
      "name": "reference",
      "label": "As"
    }
  ],
  "yields": "NONE"
}